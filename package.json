{
	"name": "react-antd-admin",
	"type": "module",
	"version": "0.0.0",
	"private": true,
	"packageManager": "pnpm@10.26.2",
	"description": "React Hooks + TypeScript + Ant Design = react-antd-admin",
	"author": "Condor Hero",
	"license": "MIT",
	"homepage": "https://github.com/condorheroblog/react-antd-admin/blob/main/README.md",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/condorheroblog/react-antd-admin.git"
	},
	"bugs": {
		"url": "https://github.com/condorheroblog/react-antd-admin/issues"
	},
	"imports": {
		"#*": "./*"
	},
	"scripts": {
		"dev": "vite --host",
		"build": "NODE_OPTIONS=--max-old-space-size=8192 vite build",
		"preview": "vite preview",
		"typecheck": "tsc --noEmit",
		"analyzer": "vite-bundle-visualizer --output analyzer/stats.html",
		"eslint-config-inspector": "eslint-config-inspector",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"prepare": "simple-git-hooks",
		"pnpm:install": "pnpm install",
		"npm-check": "taze major -lrw",
		"check:circular-deps": "ds src --output circular-deps.json",
		"test": "vitest"
	},
	"dependencies": {
		"@ant-design/icons": "^6.1.0",
		"@ant-design/pro-components": "^3.1.0-0",
		"@dnd-kit/core": "^6.3.1",
		"@dnd-kit/sortable": "^10.0.0",
		"@dnd-kit/utilities": "^3.2.2",
		"@tanstack/react-query": "^5.90.14",
		"ahooks": "^3.9.6",
		"antd": "^6.1.2",
		"antd-img-crop": "^4.27.0",
		"dayjs": "^1.11.19",
		"echarts": "^6.0.0",
		"echarts-for-react": "^3.0.5",
		"i18next": "^25.7.3",
		"keepalive-for-react": "^5.0.7",
		"ky": "^1.14.2",
		"motion": "^12.23.26",
		"nprogress": "^0.2.0",
		"pinyin-pro": "^3.27.0",
		"react": "^19.2.3",
		"react-countup": "^6.5.3",
		"react-dom": "^19.2.3",
		"react-error-boundary": "^6.0.1",
		"react-i18next": "^16.5.0",
		"react-jss": "^10.10.0",
		"react-router": "^7.11.0",
		"simplebar-react": "^3.3.2",
		"spin-delay": "^2.0.1",
		"tailwind-merge": "^3.4.0",
		"zustand": "^5.0.9"
	},
	"devDependencies": {
		"@ant-design/cssinjs": "^2.0.1",
		"@antfu/eslint-config": "^6.7.3",
		"@commitlint/cli": "^20.2.0",
		"@commitlint/config-conventional": "^20.2.0",
		"@eslint-react/eslint-plugin": "^2.4.0",
		"@eslint/config-inspector": "^1.4.2",
		"@faker-js/faker": "^10.1.0",
		"@iconify-json/ri": "^1.2.6",
		"@iconify/tools": "^5.0.1",
		"@svgr/plugin-jsx": "^8.1.0",
		"@svgr/plugin-svgo": "^8.1.0",
		"@tailwindcss/vite": "^4.1.18",
		"@tanstack/react-query-devtools": "^5.91.2",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/react": "^16.3.1",
		"@testing-library/user-event": "^14.6.1",
		"@types/node": "^25.0.3",
		"@types/nprogress": "^0.2.3",
		"@types/react": "^19.2.7",
		"@types/react-dom": "^19.2.3",
		"@vitejs/plugin-react": "^5.1.2",
		"circular-dependency-scanner": "^3.0.1",
		"clsx": "^2.1.1",
		"code-inspector-plugin": "^1.3.4",
		"eslint": "^9.39.2",
		"eslint-plugin-react-hooks": "^7.0.1",
		"eslint-plugin-react-refresh": "^0.4.26",
		"globals": "^16.5.0",
		"happy-dom": "^20.0.11",
		"lint-staged": "^16.2.7",
		"simple-git-hooks": "^2.13.1",
		"tailwindcss": "^4.1.18",
		"taze": "^19.9.2",
		"typescript": "^5.9.3",
		"unplugin-icons": "^22.5.0",
		"vite": "^7.3.0",
		"vite-bundle-visualizer": "^1.2.1",
		"vite-plugin-checker": "^0.12.0",
		"vite-plugin-fake-server": "^2.2.2",
		"vite-plugin-svgr": "^4.5.0",
		"vitest": "^4.0.16"
	},
	"simple-git-hooks": {
		"pre-commit": "npx lint-staged",
		"commit-msg": "npx commitlint --edit"
	},
	"lint-staged": {
		"*": [
			"eslint --fix"
		]
	}
}
